    <connection>Conexion_SQL</connection>
    <sql>SELECT 
codigo_grupo_de_paises as Codigo_sub_continente,
Grupo_de_paises as Nombre_sub_continente,
case 
 when Grupo_de_paises like '%África%' then '5100'
 when Grupo_de_paises like '%América%' or  Grupo_de_paises like '%Carib%' and '5299' then '5200'
 when Grupo_de_paises like '%Asia%' and '5399' then '5300'
 when Grupo_de_paises like '%Europa%' and '5499' then '5400'
 when Grupo_de_paises like '%Oceanía%' and '5599' then '5500'
 when Grupo_de_paises like '%Región Antártica%' and '5699' then '5600' else '5100' end as codigo_continente
 from stg1_grupo_pais stgp
where codigo_grupo_de_paises not in ('5000','5501','5502', '5503','5504') and codigo_grupo_de_paises &lt;5700
group by
codigo_grupo_de_paises, Grupo_de_paises, 
case 
 when Grupo_de_paises like '%África%' then '5100'
 when Grupo_de_paises like '%América%' or  Grupo_de_paises like '%Carib%' and '5299' then '5200'
 when Grupo_de_paises like '%Asia%' and '5399' then '5300'
 when Grupo_de_paises like '%Europa%' and '5499' then '5400'
 when Grupo_de_paises like '%Oceanía%' and '5599' then '5500'
 when Grupo_de_paises like '%Región Antártica%' and '5699' then '5600'
 else '5100'
end

</sql>
